	epd_bitmap_anim_frame_2,
	epd_bitmap_anim_frame_3,
	epd_bitmap_anim_frame_4
};

int frame = 0;      // frame for the inner part of the icon


void setup() {
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  display.clearDisplay();
}

void loop() {
  display.clearDisplay();
  display.drawBitmap(0,0, epd_bitmap_allArray[frame], 128, 64, WHITE);
  display.display();
  frame = frame+1;            // increase the frame for the inner part of the icon
  if (frame > 3) {frame = 0;} // there are total of 3 frames for the animation
  delay(100);
}
